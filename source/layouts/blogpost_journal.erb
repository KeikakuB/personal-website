<% wrap_layout :layout do %>
    <% content_for :header do %>
        <%= partial "feed_journals" %>
    <% end %>
    <div class="container">
        <div class="row justify-content-center">
            <h1>
                <%= current_page.data.title %>
            </h1>
        </div>
        <%= page_links(current_page) %>
        <div class="row justify-content-center">
          <div class="col-12 col-sm-10 col-md-9 col-lg-8">
              <div class="row">
                  <%= yield %>
              </div>
          </div>
        </div>
    </div>
<% end %>
