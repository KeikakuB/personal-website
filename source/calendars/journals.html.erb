---
pageable: false
blog: journals
---
<% content_for :head do %>
    <%= partial "feed_journals" %>
<% end %>
<% content_for :sidebar do %>
    <%= partial "sidebar_journals" %>
<% end %>
<% case page_type
   when 'day' %>
  <% date = Date.new(year, month, day).strftime('%b %e %Y') %>
<% when 'month' %>
  <% date = Date.new(year, month, 1).strftime('%b %Y') %>
<% when 'year' %>
  <% date = year.to_s %>
<% end %>
<%= partial "page_number" %>
<%= partial "page_previous" %>
<%= articles_table(articles, "Blog posts from " + date) %>
<%= partial "page_next" %>
